#select_chart
  #select_date
    = form_tag game_online_select_url, method: :post, remote: true do
      %span#title Choose day:
      = select_date params[:date]
      = submit_tag 'Renew'
  - if @game_online_select
    #carrency
      %h4#title Period data:
      %span.name Average:
      %span#average 0
      %br
      %span.name Max:
      %span#max 0
      %br
      %span.name Min:
      %span#min 0
    #chart
      = high_chart 'game_online_select', @game_online_select
  - else
    %h1#fail Period no have data!